#!/bin/sh
#
# Copyright (C) 2015 OpenWrt.org
#

. /lib/functions/uci-defaults.sh

board_config_update

board=$(board_name)

case "$board" in
iyunlink,m87x2)
	ucidef_set_led_netdev "wan-green" "WAN Green LED" "iyunlink:green:wan" "eth1" "link" "tx rx"
	ucidef_set_led_netdev "wan-yellow" "WAN Yellow LED" "iyunlink:yellow:wan" "eth1" "link" "tx rx"
	;;
esac

board_config_flush

exit 0
